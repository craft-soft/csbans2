{% extends 'base.twig' %}
{% block content %}
    {% if models|length %}
        <dl class="row">
            {% for model in models %}
                <dt class="col-sm-6 col-lg-5">{{ t('admin/params', model.label) }}</dt>
                <dd class="col-sm-6 col-lg-7">{{ model.viewValue }}</dd>
            {% endfor %}
        </dl>
    {% endif %}
{% endblock %}